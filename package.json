{
  "name": "hidden-attr",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel",
    "clean": "rm -rf dist",
    "lighthouse": "lighthouse \"http://localhost:5000\" --chrome-flags=\"--headless\" --output json --output-path ./results/\"$(git rev-parse --abbrev-ref HEAD)\"/\"$(date +%Y-%m-%d_%H-%M-%S.json)\"",
    "build": "parcel build src/index.html --public-url ./",
    "build:audit": "yarn clean && yarn build && concurrently --kill-others \"yarn build:serve\" \"yarn lighthouse\"",
    "build:serve": "yarn serve -s ./dist",
    "deploy": "yarn clean && yarn build && surge --domain https://hidden-attribute.surge.sh --project ./dist"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "7.0.0",
    "parcel": "2.3.1"
  },
  "dependencies": {
    "axios": "0.25.0",
    "overlayscrollbars": "1.13.1",
    "react": "0.0.0-experimental-64223fed8-20220210",
    "react-dom": "0.0.0-experimental-64223fed8-20220210",
    "recharts": "2.1.9",
    "victory": "36.2.1"
  },
  "alias": {
    "process": {
      "global": "process"
    }
  }
}
